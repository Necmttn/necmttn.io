  "scripts": {
    "build": "export ENV=production && yarn i18n:compile && gatsby build",
    "clean": "rimraf public",
    "dev": "yarn i18n:compile && gatsby develop",
    "lint": "tslint -t stylish -p .",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix 'src/**/*.ts' 'test/**/*.ts'",
    "start": "npm run dev",
    "i18n:compile": "node src/content/i18n/compile.js",
    "i18n:watch": "nodemon --watch src/content/i18n  -e csv src/content/i18n/compile.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
